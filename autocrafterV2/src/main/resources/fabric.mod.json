{
	"schemaVersion": 1,
	"id": "autocrafter",
	"version": "${version}",
	"name": "Autocrafter",
	"description": "A simple Autocrafter mod which works via a template.",
	"authors": [
		"Jakob Sprenger"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/Jukub/Autocrafter"
	},
	"license": "MIT",
	"icon": "assets/autocrafter/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.jsp.autocrafter.Autocrafter"
		],
		"fabric-datagen": [
			"net.jsp.autocrafter.AutocrafterDataGenerator"
		],
		"client": [
			"net.jsp.autocrafter.AutocrafterClient"
		]
	},
	"mixins": [
		"autocrafter.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.2",
		"minecraft": "~1.21.1",
		"java": ">=21",
